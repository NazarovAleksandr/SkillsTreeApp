{
    "parser": "babel-eslint",
    "extends": [
        "airbnb"
    ],
    "env": {
        "browser": true
    },
    "rules": {
        "jsx-quotes": [
        1,
        "prefer-double"
        ],
        "linebreak-style": "off",
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4]
    }
}